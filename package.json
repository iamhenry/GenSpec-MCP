{
  "name": "genspec-mcp",
  "version": "1.0.0",
  "description": "MCP server for GenSpec workflow - converts USER-STORIES.md into README, ROADMAP, and SYSTEM-ARCHITECTURE documents",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "documentation",
    "genspec",
    "readme",
    "roadmap",
    "architecture"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "tsx": "^4.0.0"
  },
  "files": [
    "dist/",
    "templates/",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}